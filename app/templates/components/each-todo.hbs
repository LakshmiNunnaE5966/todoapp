<li {{bind-attr class="todo.completed:true"}}>
    <div class="check">{{input class="check" type="checkbox" checked=todo.completed}}</div>
    {{#if checked}}
    <div class="title"><h4><s>{{title}}</s></h4></div>
    {{else}}
    <div><i>{{fa-icon "minus" size=2 border=true}}</i></div>
    <div class="title"><h4>{{title}}</h4></div>
    {{/if}}
    <div><i>{{fa-icon "trash"size=2 }}</i></div>
</li>
